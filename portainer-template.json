[
  {
    "title": "Node ",
    "description": "Node  on d8.8",
    "logo": "https://nodejs.org/static/images/logos/nodejs-new-pantone-black.png",
    "image": "davask/d-node:6-d8.8",
    "ports": [
        "22/tcp",
        "80/tcp",
        "443/tcp",
        "35729/tcp",
        "8080/tcp"
    ],
    "env": [
        {
            "name": "DWL_LOCAL_LANG",
            "label": "Local lang [en_US:en]"
        },{
            "name": "DWL_LOCAL",
            "label": "Local [en_US.UTF-8]"
        },{
            "name": "DWL_USER_NAME",
            "label": "Username [username]"
        },{
            "name": "DWL_USER_PASSWD",
            "label": "User password for ssh access [secret]"
        },{
            "name": "DWL_USER_ID",
            "label": "User id [1000]"
        },{
            "name": "DWL_SSH_ACCESS",
            "label": "Enable ssh access [false]"
        }
    ],
    "volumes": [
        "/home/host"
    ]
  }
]
